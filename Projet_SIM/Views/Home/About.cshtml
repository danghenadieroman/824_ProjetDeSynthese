@model IEnumerable <Projet_SIM.Models.Utilisateur>

@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>
@{  
    foreach (var item in Model)
    {
        @item.nomUtilisateur <br>
        @item.motDePasse <br>

    }
}
